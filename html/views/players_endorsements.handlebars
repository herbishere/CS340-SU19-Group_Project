<h1>Welcome to the Players / Endorsements Page!</h1>
<h2>View Endorsements by Player</h2>
<!-- DISPLAY ALL THE PLAYERS/Endorsements IN A TABLE -->
<div class="table display player_endorsements">
    <h2>Endorsements</h2>
    <table>
        <thead>
            <tr>
                <th>Player ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Endorsement ID</th>
            </tr>
        </thead>
        <tbody>
            {{#each playerEndorsements}}
            <tr >
                <td>{{player_ID}}</td>
                <td>{{first_name}}</td>
                <td>{{last_name}}</td>
                <td>{{endorsement_ID}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>